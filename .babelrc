{
  presets: [
    ["env", {
      "targets": {
        "browsers": ["ie >= 9"]
      },
      "modules": "commonjs",
      "debug": false,
      "useBuiltIns": true
    }],
    "stage-0"
  ],
  plugins: [
    ["component", [{
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      },
      {
        "libraryName": "mint-ui",
        "style": true
      }
    ]],
    ["import", {
      "libraryName": "vant",
      "libraryDirectory": "es",
      "style": true
    }],
    "webpack-async-module-name",
    "transform-runtime",
    "ynw"
  ]
}